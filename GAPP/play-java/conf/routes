# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Connexion
GET     /                           controllers.Connexion.login
POST    /                           controllers.Connexion.authenticate
GET     /logout                     controllers.Connexion.logout

GET     /see/competences            controllers.Test.index

GET     /sign                       controllers.Sign.signIn
POST    /sign                       controllers.Sign.create

#Competence
GET     /ajout/competence           controllers.Competences.addCompetences
POST    /ajout/competence           controllers.Competences.create

# Deadline
GET    /deadline					controllers.Deadline.deadline

# Profil
GET    /mon/profil					controllers.Profil.profil

# Recherche
GET    /recherche					controllers.Recherche.recherche

#Fiche
GET    /fiche						controllers.Fiche.fiches





# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
